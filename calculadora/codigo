    double numero = -1;
    char sinal;
    StringBuilder numeroTemp = new StringBuilder();
    private void operacao(){
    
        switch(sinal){
            
            case '*':
                numero = numero * (Double.parseDouble(txtTela.getText()));
                break;
            case '/':
                numero = numero / (Double.parseDouble(txtTela.getText()));
                break;
            case '+':
                numero = numero + (Double.parseDouble(txtTela.getText()));
                break;
            case '-':
                numero = numero - (Double.parseDouble(txtTela.getText()));
            
        }
        
    }

