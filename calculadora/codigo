    private void btnDividirActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
        if (numero == -1){
            
            numero = Double.parseDouble(txtTela.getText());
            sinal = '/';
            lblNumeros.setText(numero+"/");
            txtTela.setText("");
            
        } else{
            
            operacao();
            sinal = '*';
            lblNumeros.setText(numero+"/");
            txtTela.setText("");
            
        }
    }                                          

    private void btnMenosActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        if (numero == -1){
            
            numero = Double.parseDouble(txtTela.getText());
            sinal = '-';
            lblNumeros.setText(numero+"-");
            txtTela.setText("");
            
        } else{
            
            operacao();
            sinal = '-';
            lblNumeros.setText(numero+"-");
            txtTela.setText("");
            
        }
    }                                        

    private void btnMaisActionPerformed(java.awt.event.ActionEvent evt) {                                        
        // TODO add your handling code here:
        if (numero == -1){
            
            numero = Double.parseDouble(txtTela.getText());
            sinal = '+';
            lblNumeros.setText(numero+"+");
            txtTela.setText("");
            
        } else{
            
            operacao();
            sinal = '+';
            lblNumeros.setText(numero+"+");
            txtTela.setText("");
            
        }
    }                                       

    private void btnPontoActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        txtTela.setText(txtTela.getText() + '.');
    }                                        

    private void btnIgualActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        operacao();
        lblNumeros.setText("");
        txtTela.setText(numero+"");

    }                                        

    private void btnMultiplicarActionPerformed(java.awt.event.ActionEvent evt) {                                               
        // TODO add your handling code here:
        if (numero == -1){
            
            numero = Double.parseDouble(txtTela.getText());
            sinal = '*';
            lblNumeros.setText(numero+"*");
            txtTela.setText("");
            
        } else{
            
            operacao();
            sinal = '*';
            lblNumeros.setText(numero+"*");
            txtTela.setText("");
            
        }
    }                                              

    private void btnCEActionPerformed(java.awt.event.ActionEvent evt) {                                      
        // TODO add your handling code here:
        lblNumeros.setText("");
        txtTela.setText("");
        numero = -1;
    }                                     

    private void btnC1ActionPerformed(java.awt.event.ActionEvent evt) {                                      
        // TODO add your handling code here:
        numeroTemp.append(txtTela.getText());
        numeroTemp.deleteCharAt(numeroTemp.length()-1);
        txtTela.setText(numeroTemp.toString());
        numeroTemp.setLength(0);

    }                                     

